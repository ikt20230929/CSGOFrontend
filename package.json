{
  "name": "csgofrontend",
  "version": "1.0.0",
  "description": "CSGO Frontend",
  "main": "index.js",
  "scripts": {
    "build:esbuild": "node build.mjs build",
    "watch:esbuild": "node build.mjs watch",
    "cert": "node build.mjs cert",
    "config": "ncp config/settings-prod.js src/js/settings.js --stopOnErr",
    "config:dev": "ncp config/settings-dev.js src/js/settings.js --stopOnErr",
    "config:dev:watch": "nodemon --exec \"ncp config/settings-dev.js src/js/settings.js --stopOnErr\" --watch config/settings-dev.js",
    "serve": "serve public/ -s --ssl-cert localhost.crt --ssl-key localhost.key",
    "open": "opener https://localhost:3000",
    "build": "npm run config && npm run build:esbuild",
    "test": "jest",
    "testEnvironment": "jsdom",
    "build:dev": "npm run config:dev && npm run build:esbuild",
    "watch": "npm run cert && concurrently --kill-others-on-fail npm:config:dev:watch npm:watch:esbuild npm:serve npm:open"
  },
  "browserslist": "defaults, not IE 11, not dead",
  "homepage": "./",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.2",
    "esbuild-sass-plugin": "^3.2.0",
    "execa": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "ncp": "^2.0.0",
    "node-forge": "^1.3.1",
    "nodemon": "^3.1.0",
    "opener": "^1.5.2",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "serve": "^14.2.3"
  },
  "dependencies": {
    "@mantine/core": "^7.9.1",
    "@mantine/dates": "^7.9.1",
    "@mantine/form": "^7.9.1",
    "@mantine/hooks": "^7.9.1",
    "@mantine/nprogress": "^7.9.1",
    "@reduxjs/toolkit": "^2.2.4",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux-remember": "^5.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
