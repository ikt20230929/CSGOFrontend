{
  "name": "csgofrontend",
  "version": "1.0.0",
  "description": "CSGO Frontend",
  "main": "index.js",
  "scripts": {
    "build:esbuild": "node build.mjs build",
    "watch:esbuild": "node build.mjs watch",
    "config": "ncp config/settings-prod.js src/js/settings.js --stopOnErr",
    "config:dev": "ncp config/settings-dev.js src/js/settings.js --stopOnErr",
    "config:dev:watch": "nodemon --exec \"ncp config/settings-dev.js src/js/settings.js --stopOnErr\" --watch config/settings-dev.js",
    "serve": "serve public/ -s",
    "open": "opener http://localhost:3000",
    "build": "npm run config && npm run build:esbuild",
    "build:dev": "npm run config:dev && npm run build:esbuild",
    "watch": "concurrently --kill-others-on-fail npm:config:dev:watch npm:watch:esbuild npm:serve npm:open",
    "cypress": "wait-on http://localhost:3000 && cypress run",
    "test": "concurrently --kill-others npm:serve npm:cypress"
  },
  "browserslist": "defaults, not IE 11, not dead",
  "homepage": "./",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.6",
    "esbuild": "0.20.1",
    "esbuild-sass-plugin": "^3.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.0",
    "opener": "^1.5.2",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "serve": "^14.2.1",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@mantine/core": "^7.5.3",
    "@mantine/form": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mantine/nprogress": "^7.5.3",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "redux-remember": "^5.1.0"
  }
}
