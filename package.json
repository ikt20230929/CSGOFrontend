{
  "name": "csgofrontend",
  "version": "1.0.0",
  "description": "CSGO Frontend",
  "main": "index.js",
  "scripts": {
    "build:esbuild": "esbuild src/js/index.js --bundle --minify --platform=node --loader:.js=jsx --format=cjs --outfile=public/dist/bundle.js",
    "watch:esbuild": "esbuild src/js/index.js --bundle --platform=node --loader:.js=jsx --format=cjs --outfile=public/dist/bundle.js --watch",
    "serve": "serve public/ -s",
    "open": "opener https://ikt.14c-on.top/",
    "build": "cross-env NODE_ENV=production concurrently --kill-others-on-fail npm:build:esbuild",
    "build:dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail npm:build:esbuild",
    "watch": "cross-env NODE_ENV=development concurrently --kill-others-on-fail npm:watch:esbuild npm:serve npm:open"
  },
  "homepage": "./",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.1",
    "esbuild": "0.19.4",
    "opener": "^1.5.2",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.11.0",
    "postcss-simple-vars": "^7.0.1",
    "serve": "^14.2.1"
  },
  "dependencies": {
    "@mantine/core": "^7.2.2",
    "@mantine/hooks": "^7.2.2",
    "@mantine/nprogress": "^7.2.2",
    "axios": "^1.6.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.18.0"
  }
}
